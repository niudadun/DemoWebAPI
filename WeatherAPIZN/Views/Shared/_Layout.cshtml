<!DOCTYPE html>
<html id="partialview">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Zheng Niu WeatherAPIDemo Application</title>
    @Styles.Render("~/Content/css")
    @*<script src="~/Scripts/Fntest.js"></script>*@
    @*@RenderSection("Content", false)*@
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/customjs")
    @*@RenderSection("scripts", required: false)*@

</head>
<body id="bgcontent">
    <div id="mainnavabar" class="navbar navbar-inverse navbar-fixed-top">
        <div class="container-full">
            <div class="navbar-header page-scroll">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Weather API - Zheng Niu", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    @*<li>@Html.ActionLink("Search Settings", "Index", "Search")</li>*@
                    @*<li><a id="registerInvoke" href="#">Register</a></li>*@
                    @*<li>@Html.ActionLink("Home", "Index", "Home")</li>*@
                </ul>
            </div>
        </div>
    </div>
    <header>
        <div id="editview" class="container-full">
            @RenderBody()
        </div>
    </header>


</body>
</html>
<script>
    var siteRoot = '@Request.ApplicationPath';
    if (siteRoot == "/") siteRoot = window.location.origin+"/";
</script>
